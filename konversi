#Konversi Jenis Bilangan Int ke Float
print(int(1.5))
print(int(2.5))
print(float(3.5))

#Python Decimal
print((2.2 + 3.3) == 4.4)
print(2.2 + 3.3)

#Bilangan Pecahan
import fractions
#output : 3/2
print(fractions.Fraction(1.5))
print(fractions.Fraction(1,3))

from fractions import Fraction as f
print(f(1,4) + f(1,3))
print(1/ f(3,5))
print(f(-2,8) < 0)
